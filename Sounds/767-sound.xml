                    <avionics>
			<name>avionics</name>
			<path>Aircraft/767-300/Sounds/avionics.wav</path>
			<mode>looped</mode>
			<condition>
				<property>sim/current-view/internal</property>
				<greater-than>
			<property>systems/electrical/outputs/efis</property>
					<value>0</value>
				</greater-than>
			</condition>
			<position>
				<x>-17</x>
				<y>0.01</y>
				<z>-1.95</z>
			</position>	
			<volume>
				<factor>0.2</factor>
				<max>0.5</max>
			</volume>
			<reference-dist>1.2</reference-dist>
			<max-dist>5</max-dist>
		</avionics>

        <appr_mins>
        <name>appr_mins</name>
        <mode>once</mode>
        <type>avionics</type>		
        <path>Aircraft/767-300/Sounds/approaching-minimums.wav</path>
        <condition>
			<less-than>
				<property>/position/altitude-agl-ft</property>
				<value>380</value>
			</less-than>
			<greater-than>
				<property>/position/altitude-agl-ft</property>
				<value>300</value>
			</greater-than>			
			<less-than>
				<property>/velocities/vertical-speed-fps</property>
				<value>0</value>
			</less-than>
		</condition>
          </appr_mins>
	   
		
