    <engine>
        <name>rightwhine2</name>
        <path>Aircraft/767-300/Sounds/whine2.wav</path>
        <mode>looped</mode>
        <property>engines/engine[0]/rpm</property>
    <condition>
      <and>
<not>
        <greater-than>
          <property>/position/altitude-ft</property>
          <value>500</value>
        </greater-than>
</not>
      </and>
    </condition>
        <pitch>
            <property>/engines/engine[0]/rpm</property>
            <offset>0</offset>
            <factor>0.01</factor>
            <min>0.0</min>
            <max>1.0</max>
        </pitch>
        <volume>
            <property>engines/engine[0]/rpm</property>
            <factor>0.1</factor>
            <min>0.0</min>
            <max>0.6</max>
        </volume>
        <volume>
            <property>sim/sound/Evolume</property>
