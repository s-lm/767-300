    <engine>
        <name>right</name>
        <path>Aircraft/767-300/Sounds/whine.wav</path>
        <mode>looped</mode>
        <volume>
            <property>engines/engine[1]/rpm</property>
            <factor>0.1</factor>
            <min>0.0</min>
            <max>0.6</max>
        </volume>
        <volume>
            <property>sim/sound/Evolume</property>
        </volume>
        <pitch>
            <property>/engines/engine[1]/rpm</property>
            <offset>0</offset>
            <factor>0.01</factor>
            <min>0.0</min>
